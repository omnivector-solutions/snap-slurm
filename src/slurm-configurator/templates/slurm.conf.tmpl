ClusterName="SLURM-SNAP-LOCAL"
PlugStackConfig="/var/snap/slurm/common/etc/slurm/plugstack.conf"

SlurmdPort=6818
SlurmctldPort=6817

SlurmUser="root"
SlurmdUser="root"


AuthType="auth/munge"
AuthInfo="socket=/tmp/munged.socket.2"

DefaultStorageType=filetxt


JobAcctGatherType="jobacct_gather/linux"
